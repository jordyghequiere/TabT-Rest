{
  "name": "tabt-rest",
  "version": "1.12.95",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": "18.16.x",
    "npm": "9.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "node ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test-e2e/**/*.ts\\\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-badge-generator",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config test-e2e/jest-e2e.json",
    "release": "release-it"
  },
  "dependencies": {
    "@nestjs/axios": "1.0.1",
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.12",
    "@nestjs/platform-express": "9.3.12",
    "@nestjs/schedule": "2.2.1",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@xmldom/xmldom": "0.8.7",
    "bluebird": "3.7.2",
    "cache-manager": "5.2.0",
    "cache-manager-memory-store": "1.1.0",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "form-data": "4.0.0",
    "he": "1.2.0",
    "helmet": "6.0.1",
    "jsdom": "20.0.3",
    "lodash": "4.17.21",
    "lunr": "2.3.9",
    "nestjs-pino": "3.1.2",
    "pino-http": "8.3.3",
    "pino-multi-stream": "6.0.0",
    "pino-pretty": "9.4.0",
    "reflect-metadata": "0.1.13",
    "response-time": "2.3.2",
    "rxjs": "7.8.0",
    "soap": "1.0.0",
    "socks-proxy-agent": "7.0.0",
    "swagger-ui-express": "4.6.2",
    "user-agents": "1.0.1352",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.12",
    "@openapitools/openapi-generator-cli": "2.6.0",
    "@release-it/conventional-changelog": "5.1.1",
    "@types/bluebird": "3.5.38",
    "@types/compression": "1.7.2",
    "@types/continuation-local-storage": "3.2.4",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.17",
    "@types/he": "1.2.0",
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.194",
    "@types/lunr": "2.3.4",
    "@types/node": "18.15.11",
    "@types/response-time": "2.3.5",
    "@types/supertest": "2.0.12",
    "@types/user-agents": "1.0.2",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-badge-generator": "1.1.5",
    "prettier": "2.8.7",
    "release-it": "15.9.3",
    "rimraf": "3.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      "entity",
      ".contract.ts",
      ".constants.ts",
      ".decorator.ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "json-summary",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
